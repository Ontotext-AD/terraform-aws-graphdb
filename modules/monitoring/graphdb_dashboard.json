{
  "widgets": [
      {
          "height": 6,
          "width": 6,
          "y": 0,
          "x": 0,
          "type": "metric",
          "properties": {
              "metrics": [
                  [ "AWS/Route53", "HealthCheckPercentageHealthy", "HealthCheckId", "${health_check_id}", { "id": "m1", "region": "${route53_availability_check_region}" } ]
              ],
              "period": 300,
              "stat": "Average",
              "region": "${route53_availability_check_region}",
              "title": "Availability Tests",
              "yAxis": {
                  "left": {
                      "max": 100,
                      "min": 0
                  }
              }
          }
      },
      {
          "height": 6,
          "width": 6,
          "y": 0,
          "x": 6,
          "type": "metric",
          "properties": {
              "metrics": [
                  [ { "expression": "SELECT AVG(CPUUtilization) FROM \"AWS/EC2\" GROUP BY InstanceId", "label": "Query1", "id": "q1", "region": "${aws_region}" } ]
              ],
              "region": "${aws_region}",
              "stacked": false,
              "view": "timeSeries",
              "period": 300,
              "stat": "Average",
              "yAxis": {
                  "left": {
                      "max": 100,
                      "label": "Percent",
                      "showUnits": false
                  },
                  "right": {
                      "label": "",
                      "showUnits": false
                  }
              },
              "title": "CPU utilization per instance"
          }
      },
      {
        "height": 6,
        "width": 6,
        "y": 0,
        "x": 12,
        "type": "metric",
        "properties": {
            "metrics": [
                [ { "expression": "SELECT AVG(mem_used_percent) FROM \"CWAgent\" GROUP BY host", "label": "Query1", "id": "q1", "region": "${aws_region}" } ]
            ],
            "region": "${aws_region}",
            "stacked": false,
            "view": "timeSeries",
            "period": 300,
            "stat": "Average",
            "yAxis": {
                "left": {
                    "max": 100,
                    "label": "Percent",
                    "showUnits": false
                },
                "right": {
                    "label": "",
                    "showUnits": false
                }
            },
            "title": "GraphDB Memory Used % per instance"
        }
    },
      {
