              "stacked": false,
              "view": "timeSeries",
              "period": 300,
              "stat": "Average",
              "yAxis": {
                  "left": {
                      "label": "Gigabytes",
                      "showUnits": false
                  },
                  "right": {
                      "label": "",
                      "showUnits": false
                  }
              },
              "title": "GraphDB Data Dir Free per instance"
          }
      },
